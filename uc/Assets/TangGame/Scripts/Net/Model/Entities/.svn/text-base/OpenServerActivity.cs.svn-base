using System;

namespace TangGame.Net
{
/// <summary>
/// 开服活动
/// </summary>
public class OpenServerActivity
{
    /** 活动Id */
    public short id;
    /** 活动名称 */
    public string name;
    /** 活动类型 */
    public short type;
    /** 开启活动时间(开服后天数) */
    public int startDate;
    /** 活动有效时间(开服后天数) */
    public int validateDate;
    /** 是否发送广播 */
    public bool isNotice;
    /** 公告Id */
    public short noticeId;
    /** 条件 */
    public int[][] conditionArray;
    /** 奖励 */
    public int[][] rewardArray;
}
}
