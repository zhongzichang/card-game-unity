using System;
using System.Collections.Generic;
using UnityEngine;

namespace TangGame.Net
{
/** Hero简单消息(主要用于登陆获取玩家角色列表) */
public class SimpleHero
{
    /** Hero ID */
    public long heroId;
    /** 名称 */
    public String name;
    /** 地图ID */
    public short gameMapId;
    /** 性别(1:男 2:女) */
    public short sex;
    /** 流派(1，无限制 2，凌烟阁 3，信陵客 4，清静楼) */
    public short genre;
    /** 头像下标 */
    public short headImg;
    /** 角色下标 */
    public short index;
    /** 等级 */
    public short level;
    /** 上次下线时间 */
    public long exitTime;
    /** 封停时间  */
    public long blokIdTime;


}
}

