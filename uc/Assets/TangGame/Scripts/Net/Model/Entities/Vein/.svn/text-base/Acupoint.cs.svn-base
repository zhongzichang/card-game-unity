using System;
using System.Collections.Generic;

namespace TangGame.Net
{
/// <summary>
/// 穴位基础类
/// </summary>
public class Acupoint
{
    /** 穴位编号 */
    public short id;
    /** 穴位名字 */
    public String name;
    /** 穴位等级 */
    public short level;
    /** 所属经脉 */
    public short veinType;
    /** 耗费内息 */
    public int useBreath;
    /** 穴位效果 */
    public List<Effect> effectList;
    /** 下一级 */
    public short nextId;
    /** 穴位效果描述 */
    public string effectDescript;
    /** 位置 */
    public short position;
    /** 具体类型 */
    public short mType;
    /** 上一级ID */
    public short upId;
    /** 是否最后一个穴位 */
    public bool isLast;
    /** 升级基础几率 */
    public int odds;
    /** 升级使用物品编号 */
    public int useGoodsId;
    /** 升级使用物品数量 */
    public int useGoodsNum;
    /** 使用物品几率 */
    public int goodsOdds;
}
}
