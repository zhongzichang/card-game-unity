using System;

namespace TangGame.Net
{
/// <summary>
/// Description of BuffBase.
/// </summary>
public class BuffBase
{
    /** 序列号 */
    public int id;
    /** 类型 */
    public short type;
    /** 持续时间(ms) */
    public int validTime;
    /** 间隔时间 */
    public int intervalTime;
    /** 值 */
    public int value;
    /** buff/debuff */
    public bool isPositive;
    /** 几率 */
    public short probability;
    /** 是否清除 */
    public bool isClear;
}
}
