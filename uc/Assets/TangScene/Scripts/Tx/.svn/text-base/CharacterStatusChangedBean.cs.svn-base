namespace TangScene
{
  public class CharacterStatusChangedBean
  {
    public long actorId;
    public CharacterStatus before;
    public CharacterStatus after;

    public CharacterStatusChangedBean(long actorId, CharacterStatus before, CharacterStatus after)
    {
      this.actorId = actorId;
      this.before = before;
      this.after = after;
    }
  }
}