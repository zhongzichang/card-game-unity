    public delegate event LoadSuccess(int id);


    /// <summary>
    ///   加载失败后会发出该事件
    /// </summary>
    public delegate event LoadFailure(int id);
    

    /// <summary>
    ///   加载指定子场景
    /// </summary>
    public static void Load(int id)
    {
      
    }


  }
}