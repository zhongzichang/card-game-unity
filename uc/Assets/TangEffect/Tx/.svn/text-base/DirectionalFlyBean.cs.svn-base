using UnityEngine;

namespace TangEffect
{
  public class DirectionalFlyBean
  {
    public long actorId;
    public Vector3 direction;
    public float speed;
    public float distance;
    public float radius;
    public int assetSetId;
    public int assetId;


    public DirectionalFlyBean(long actorId, Vector3 direction,
			      float speed, float distance, float radius,
			      int assetSetId, int assetId)
    {
      this.actorId = actorId;
      this.direction = direction;
      this.speed = speed;
      this.distance = distance;
      this.radius = radius;
      this.assetSetId = assetSetId;
      this.assetId = assetId;
    }

  }
}
