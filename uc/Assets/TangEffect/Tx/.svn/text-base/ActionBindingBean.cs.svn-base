using UnityEngine;
using TangScene;

namespace TangEffect
{
  public class ActionBindingBean
  {
    public long actorId;
    public int assetSetId;
    public int assetId;

    public ActionBindingBean(long actorId, int assetSetId, int assetId)
    {
      this.actorId = actorId;
      this.assetSetId = assetSetId;
      this.assetId = assetId;
    }

  }
}